#!/usr/bin/env unitx

def t1() {
	expect(52 + 11{月}, 63{月})
	expect(52 - 11{月}, 41{月})
	expect(52 * 11{月}, 572{月})
	expect(52 / 11{月}, 4{月})
}

def t2() {
	expect(25 + 174{月} * 89 - 91, 15420{月})
	expect(25 / 174{月} + 89 * 91{月}, 8099{月})
}

def t3() {
	expect(25 + (174{月} * 89) - 91, 15420{月})
	expect(25 / 174{月} + (89 * 91{月}), 8099{月})
}

def t4() {
	expect(25{月} * 2{億/月}, 50{億})
	expect(24{億} / 2{億/ヶ月}, 12{ヶ月})
	expect(1,2) #Bug
}

def main() {
	t1()
	t2()
	t3()
	t4()
}

main()
