def t1() {
	assert(52 + 11{月}, 63{月})
	assert(52 - 11{月}, 41{月})
	assert(52 * 11{月}, 572{月})
	assert(52 / 11{月}, 4{月})

}

def t2() {
	assert(25 + 174{月} * 89{月} - 91, 15420{月})
	assert(25 / 174{月} + 89 * 91{月}, 8099{月})
}

def t3() {
	assert(25 + (174{月} * 89{月}) - 91, 15420{月})
	assert(25 / 174{月} + (89 * 91{月}), 8099{月})
}

def t4() {
	assert(25{月} * 2{億/月}, 50{億})
	assert(25{月} / 2{億/月}, 50{億})
}

def main() {
	print 'four ope.'
	t1()
	t2()
	t3()
	t4()
}
